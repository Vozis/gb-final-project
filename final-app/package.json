  "scripts": {
    "start": "nx run-many --target=serve --all",
    "start:production": "nx run-many --target=serve --all --configuration=production",
    "front": "nx serve web",
    "front:docker": "nx serve web --hostname=0.0.0.0",
    "front:production": "nx serve web --configuration=production",
    "server": "cd apps/server && npx prisma generate && nx serve server",
    "server:docker": "nx serve server",
    "server:production": "nx serve server --configuration=production",
    "build:server": "nx build server",
    "build:web": "nx build web",
    "build:all": "nx run-many -t  build",
    "test": "nx run-many --target=test --all",
    "prisma-generate": "cd apps/server && npx prisma generate",
    "prisma-migrate": "cd apps/server && npx prisma migrate dev",
    "prisma-reset": "cd apps/server && npx prisma db reset",
    "db:studio:docker": "env-cmd --no-override -f .env npx primsa studio --hostname 0.0.0.0"
  },
